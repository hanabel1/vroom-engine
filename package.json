{
  "name": "vroom-engine-design-system-catalog",
  "version": "0.1.0",
  "description": "Figma plugin for browsing and placing components from multiple design systems",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:ui\" \"npm:dev:plugin\"",
    "dev:ui": "vite",
    "dev:plugin": "vite --config vite.config.plugin.ts",
    "build": "npm run build:ui && npm run build:plugin",
    "build:ui": "vite build",
    "build:plugin": "vite build --config vite.config.plugin.ts",
    "validate-catalog": "tsx tools/validate-catalog.ts",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "fuse.js": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.98.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "glob": "^11.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-singlefile": "^2.0.0",
    "vitest": "^2.0.0"
  }
}
